QLineEdit {
    background-color: #bb97fc;  /* Dark Gray Background */
    color: rgb(40, 1, 82);  /* Text Color */
    border: 2px solid #555;  /* Border Color */
    min-width: 1200px;
    min-height: 30px;
    border-radius: 10px;  /* Rounded Corners */
    padding: 5px;  /* Inner Padding */
    font-size: 25px;  /* Text Size */
    padding-left: 5px;
    margin-right: 5px;
    margin-top: 250px;
}